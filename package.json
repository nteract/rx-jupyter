{
  "name": "rx-jupyter",
  "version": "1.3.0",
  "description": "RxJS 5 bindings for the Jupyter Notebook API",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:lib": "babel -d lib src --ignore '**/__tests__/**'",
    "build:lib:watch": "npm run build:lib -- --watch",
    "build:docs": "jsdoc src --recurse -d docs",
    "pretest": "npm run build",
    "test": "npm run test:unit && npm run test:lint",
    "test:unit": "mocha --compilers js:babel-core/register --recursive",
    "test:lint": "eslint .",
    "test:flow": "flow",
    "precoverage": "nyc npm run test:unit",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nteract/rx-jupyter.git"
  },
  "keywords": [
    "jupyter",
    "rxjs",
    "notebook",
    "api"
  ],
  "author": "nteract Contributors",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/rx-jupyter/issues"
  },
  "homepage": "https://github.com/nteract/rx-jupyter#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "codecov": "^2.3.1",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "flow-bin": "^0.57.3",
    "flow-copy-source": "^1.1.0",
    "flow-typed": "^2.2.0",
    "jsdoc": "^3.5.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "prettier": "^1.7.4",
    "ws": "^3.2.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "dependencies": {
    "rxjs": "^5.5.0"
  }
}
